{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/admin-dashboard/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/admin-dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "/api"
  }
}